<Window x:Name="ContactWindow" x:Class="View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:View"
        mc:Ignorable="d"
        Title="Contacts" Height="600" Width="742" RenderTransformOrigin="0.5,1" ResizeMode="NoResize">
    <Grid x:Name="ContactcsGrid" Height="NaN" Margin="15,15,15,15" Width="NaN">
        <Grid.RowDefinitions>
            <RowDefinition Height="7*"/>
            <RowDefinition Height="270*"/>
        </Grid.RowDefinitions>
        <GroupBox x:Name="EditGroupBox" Header="Edit Contact" Margin="0,0,354,28" Grid.RowSpan="2">
            <StackPanel x:Name="EditStackPanel">
                <Label Content="Name:"/>
                <TextBox Width="336" TextWrapping="Wrap" Margin="5,0,0,0" HorizontalAlignment="Left"/>
            </StackPanel>
        </GroupBox>
        <GroupBox Header="Read Contact" Margin="354,0,0,28" Grid.RowSpan="2"/>
    </Grid>
</Window>
